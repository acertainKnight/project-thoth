{
  "arxiv_machine_learning": {
    "name": "arxiv_ml",
    "source_type": "api",
    "description": "ArXiv machine learning and AI papers",
    "is_active": true,
    "api_config": {
      "source": "arxiv",
      "categories": ["cs.LG", "cs.AI", "cs.CL"],
      "keywords": ["machine learning", "neural networks", "transformer", "attention"],
      "sort_by": "lastUpdatedDate",
      "sort_order": "descending"
    },
    "schedule_config": {
      "interval_minutes": 360,
      "max_articles_per_run": 25,
      "enabled": true,
      "time_of_day": "09:00",
      "days_of_week": [0, 1, 2, 3, 4]
    },
    "query_filters": ["machine_learning", "nlp_research"]
  },

  "pubmed_ai_healthcare": {
    "name": "pubmed_ai_health",
    "source_type": "api",
    "description": "PubMed AI and healthcare papers",
    "is_active": true,
    "api_config": {
      "source": "pubmed",
      "keywords": ["artificial intelligence", "machine learning", "healthcare", "medical AI"],
      "mesh_terms": ["Artificial Intelligence", "Machine Learning", "Medical Informatics"],
      "publication_types": ["Journal Article", "Review"],
      "start_date": "2023/01/01"
    },
    "schedule_config": {
      "interval_minutes": 720,
      "max_articles_per_run": 20,
      "enabled": true,
      "time_of_day": "14:00",
      "days_of_week": [1, 3, 5]
    },
    "query_filters": ["ai_healthcare", "medical_ai"]
  },

  "nature_machine_learning": {
    "name": "nature_ml",
    "source_type": "scraper",
    "description": "Nature Machine Learning articles",
    "is_active": true,
    "scraper_config": {
      "base_url": "https://www.nature.com/subjects/machine-learning",
      "extraction_rules": {
        "title": {
          "selector": "h3.c-card__title a",
          "attribute": "text"
        },
        "authors": {
          "selector": ".c-author-list .c-author-list__item",
          "attribute": "text",
          "multiple": true
        },
        "abstract": {
          "selector": ".c-card__summary",
          "attribute": "text"
        },
        "url": {
          "selector": "h3.c-card__title a",
          "attribute": "href"
        },
        "publication_date": {
          "selector": ".c-meta__item time",
          "attribute": "datetime"
        },
        "journal": {
          "selector": ".c-meta__item .c-meta__type",
          "attribute": "text"
        }
      },
      "navigation_rules": {
        "article_container": ".c-card"
      },
      "pagination_config": {
        "enabled": true,
        "type": "link",
        "next_selector": "a[rel='next']",
        "max_pages": 3
      },
      "rate_limiting": {
        "delay": 2.0
      },
      "headers": {
        "User-Agent": "Mozilla/5.0 (compatible; ThothBot/1.0)"
      }
    },
    "schedule_config": {
      "interval_minutes": 1440,
      "max_articles_per_run": 15,
      "enabled": true,
      "time_of_day": "08:00",
      "days_of_week": [0, 2, 4]
    },
    "query_filters": ["machine_learning"]
  },

  "acm_digital_library": {
    "name": "acm_dl",
    "source_type": "scraper",
    "description": "ACM Digital Library recent papers",
    "is_active": true,
    "scraper_config": {
      "base_url": "https://dl.acm.org/action/doSearch?fillQuickSearch=false&target=advanced&ConceptID=118230",
      "extraction_rules": {
        "title": {
          "selector": ".issue-item__title a",
          "attribute": "text"
        },
        "authors": {
          "selector": ".issue-item__detail .loa__author-name",
          "attribute": "text",
          "multiple": true
        },
        "abstract": {
          "selector": ".issue-item__abstract",
          "attribute": "text"
        },
        "url": {
          "selector": ".issue-item__title a",
          "attribute": "href"
        },
        "doi": {
          "selector": ".issue-item__doi a",
          "attribute": "text",
          "transform": {
            "type": "regex",
            "pattern": "^https://doi.org/",
            "replacement": ""
          }
        },
        "publication_date": {
          "selector": ".issue-item__detail .dot-separator:last-child",
          "attribute": "text"
        }
      },
      "navigation_rules": {
        "article_container": ".issue-item"
      },
      "pagination_config": {
        "enabled": true,
        "type": "parameter",
        "page_parameter": "startPage",
        "max_pages": 5
      },
      "rate_limiting": {
        "delay": 3.0
      }
    },
    "schedule_config": {
      "interval_minutes": 2880,
      "max_articles_per_run": 30,
      "enabled": true,
      "time_of_day": "10:00",
      "days_of_week": [1, 4]
    },
    "query_filters": ["computer_science", "machine_learning"]
  },

  "ieee_xplore": {
    "name": "ieee_ai",
    "source_type": "scraper",
    "description": "IEEE Xplore AI and ML papers",
    "is_active": true,
    "scraper_config": {
      "base_url": "https://ieeexplore.ieee.org/search/searchresult.jsp?newsearch=true&queryText=artificial%20intelligence",
      "extraction_rules": {
        "title": {
          "selector": ".List-results-items .result-item-title a",
          "attribute": "text"
        },
        "authors": {
          "selector": ".List-results-items .result-item-authors .result-item-authors-info",
          "attribute": "text",
          "multiple": false,
          "transform": {
            "type": "replace",
            "old": "; ",
            "new": ", "
          }
        },
        "abstract": {
          "selector": ".List-results-items .result-item-abstract",
          "attribute": "text"
        },
        "url": {
          "selector": ".List-results-items .result-item-title a",
          "attribute": "href"
        },
        "publication_date": {
          "selector": ".List-results-items .result-item-published-date",
          "attribute": "text"
        },
        "journal": {
          "selector": ".List-results-items .result-item-publication",
          "attribute": "text"
        }
      },
      "navigation_rules": {
        "article_container": ".List-results-items"
      },
      "pagination_config": {
        "enabled": true,
        "type": "link",
        "next_selector": ".next-btn:not(.disabled)",
        "max_pages": 4
      },
      "rate_limiting": {
        "delay": 2.5
      }
    },
    "schedule_config": {
      "interval_minutes": 1440,
      "max_articles_per_run": 20,
      "enabled": true,
      "time_of_day": "16:00",
      "days_of_week": [0, 3, 5]
    },
    "query_filters": ["artificial_intelligence", "machine_learning"]
  },

  "minimal_example": {
    "name": "simple_source",
    "source_type": "api",
    "description": "Simple ArXiv source for testing",
    "is_active": true,
    "api_config": {
      "source": "arxiv",
      "categories": ["cs.AI"]
    },
    "schedule_config": {
      "interval_minutes": 60,
      "max_articles_per_run": 10,
      "enabled": true
    },
    "query_filters": []
  }
}
